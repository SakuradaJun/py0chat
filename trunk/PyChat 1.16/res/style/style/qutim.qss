 QSlider::groove:horizontal {
     height: 0px; 
border: 1px solid rgb(174, 199, 220);
     margin: 2px 0;
 }

 QSlider::handle:horizontal {
     background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(248, 249, 249, 255), stop:0.089 rgba(247, 247, 247, 255), stop:0.457 rgba(250, 249, 249, 255), stop:0.557 rgba(230, 230, 230, 255), stop:1 rgba(237, 237, 237, 255));
border: 1px solid rgb(185, 185, 185);
     width: 10px;
    min-height: 10px;
     margin: -5px 0; 
     border-radius: 5px;
 }

 QSlider::groove:vertical {

     position: absolute; 
     width: 0px; 
border: 1px solid rgb(174, 199, 220);

 }

 QSlider::handle:vertical {
     height: 10px;
max-width: 10px;
     background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(248, 249, 249, 255), stop:0.089 rgba(247, 247, 247, 255), stop:0.457 rgba(250, 249, 249, 255), stop:0.557 rgba(230, 230, 230, 255), stop:1 rgba(237, 237, 237, 255));
border: 1px solid rgb(185, 185, 185);
     margin: 0 -5px; 
     border-radius: 5px;
 }




