}


QToolButton:hover { 
border: 1px solid rgb(185, 185, 185);
background-color: qlineargradient(spread:pad, x1:0.486, y1:0, x2:0.502, y2:1, stop:0 rgba(226, 234, 241, 255), stop:0.5 rgba(237, 237, 237, 255), stop:1 rgba(226, 234, 241, 255));
